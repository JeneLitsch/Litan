function t() {
	std::print("t");
	return true;
}

function f() {
	std::print("f");
	return false;
}

function main() {
	std::println("=== And && =="); {
		var b1 = t() && t();
		std::println(" ->  true == " + b1);
		
		var b2 = t() && f();
		std::println(" ->  false == " +  b2);
		
		var b3 = f() && f();
		std::println(" ->  false == " + b3);

		var b4 = f() && t();
		std::println(" ->  false == " + b3);
	}	

	std::println("=== Or && =="); {
		var b1 = t() || t();
		std::println(" ->  true == " + b1);
		
		var b2 = t() || f();
		std::println(" ->  true == " +  b2);
		
		var b3 = f() || f();
		std::println(" ->  false == " + b3);

		var b4 = f() || t();
		std::println(" ->  true == " + b3);
	}
}