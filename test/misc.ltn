function main() {
	ltn::println("=== String ===");
	var str = "HX World";
	ltn::remove(str, 1);
	ltn::insert(str, 1, "ello");
	ltn::println(str);
	ltn::println("");

	ltn::println("=== Array ===");
	var arr = [];
	ltn::insertFront(arr, 1);
	ltn::insertBack(arr, 3);
	ltn::insert(arr, 1, 2);
	ltn::println(arr);

	const a = 123456789;
	const b = a;
	ltn::println(b);
}