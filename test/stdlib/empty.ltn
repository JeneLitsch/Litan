namespace stdlib {
    function empty(sphinx) {
        
        sphinx::run_test(sphinx, "std::empty", &std::empty(_), [
            sphinx::assert_true([[]]),
            sphinx::assert_false([[1, 2, 3]]),
            sphinx::assert_true([""]),
            sphinx::assert_false(["abc"]),
            sphinx::assert_true([std::map()]),
            sphinx::assert_fail([std::cout()]),
            sphinx::assert_fail([1]),
            sphinx::assert_fail([null]),
            sphinx::assert_fail([2.5]),
        ]);
    }
}