namespace stdlib {
	function typename(sphinx) {
		var cases = [
			sphinx::assert_result([null],                    "null"),
			sphinx::assert_result([true],                    "bool"),
			sphinx::assert_result([false],                   "bool"),
			sphinx::assert_result(['A'],                     "char"),
			sphinx::assert_result([1],                       "int"),
			sphinx::assert_result([3.14],                    "float"),
			sphinx::assert_result([[4,2]],                   "array"),
			sphinx::assert_result(["ABC"],                   "string"),
			sphinx::assert_result([std::cin()],              "istream"),
			sphinx::assert_result([std::cout()],             "ostream"),
			sphinx::assert_result([lambda => 0],             "fxptr"),
			sphinx::assert_result([std::external(0)],        "external"),
			sphinx::assert_result([std::chrono::clock()],    "clock"),
			sphinx::assert_result([std::struct()],           "struct"),
			sphinx::assert_result([std::queue()],            "queue"),
			sphinx::assert_result([std::stack()],            "stack"),
			sphinx::assert_result([std::map()],              "map"),
			sphinx::assert_result([std::random::mersenne()], "rng"),
		];

		sphinx::run_test(sphinx, "std::typename", &std::typename(_), cases);
	}
}