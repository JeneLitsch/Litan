function main(){
	std::print("Local vars: ");
	var x = 0;
	var a = 1;
	var b = 2;
	var c = 3;
	var d = 4;
	var e = 5;
	std::print(x);
	std::print(a);
	std::print(b);
	std::print(c);
	std::print(d);
	std::print(e);
	std::println("");

	std::print("Loop: ");
	for i (0, 6) {
		std::print(i);
	}
	std::println("");

	std::print("Array: ");
	var array = [];
	std::insert_back(array, 3);
	std::insert_back(array, 4);
	std::insert_back(array, 5);
	std::insert_front(array, 2);
	std::insert_front(array, 1);
	std::insert_front(array, 0);

	for i (0, std::size(array)) {
		std::print(array[i]);
	}
	std::println("");
}