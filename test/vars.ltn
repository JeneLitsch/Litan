function main(){
	ltn::print("Local vars: ");
	var x = 0;
	var a = 1;
	var b = 2;
	var c = 3;
	var d = 4;
	var e = 5;
	ltn::print(x);
	ltn::print(a);
	ltn::print(b);
	ltn::print(c);
	ltn::print(d);
	ltn::print(e);
	ltn::println("");

	ltn::print("Loop: ");
	for i (0, 6) {
		ltn::print(i);
	}
	ltn::println("");

	ltn::print("Array: ");
	var array = [];
	ltn::insertBack(array, 3);
	ltn::insertBack(array, 4);
	ltn::insertBack(array, 5);
	ltn::insertFront(array, 2);
	ltn::insertFront(array, 1);
	ltn::insertFront(array, 0);

	for i (0, ltn::size(array)) {
		ltn::print(array[i]);
	}
	ltn::println("");
}