namespace test {
	function not() {
		var cases = [
			[[true], false],
			[[false], true],
			[[null], true],
			[["Hello World"], false],
			[[2], false],
			[[0], true],
			[[-1], false],
			[[-1.5], false],
			[[-0.0], true],
		];

		var not = lambda(b) {
			return !b;
		};

		return test_all_cases("Not!", not, cases);
	}

	function null_test() {
		var x;
		var cases = [
			[[null],          false],
			[[std::null_v()], false],
			[[null],          false],
			[[x],             false],
			[[2],             true],
			[[false],         true],
			[[-1],            true],
			[[-1.5],          true],
			[[-0.0],          true],
			[["Hello World"], true],
		];

		var nul = lambda(x) {
			return ?x;
		};

		return test_all_cases("Null test ?", nul, cases);
	}

	function bool() {
		return not() + null_test();
	}
}