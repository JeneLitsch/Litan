namespace sphinx::test {
	function map_existing() {
		var map = [42:1337];
		assert_equal_strict(map[42], 1337);
	}



	function map_missing() {
		var map = [42:1337];
		assert_equal_strict(map[123], null);
	}



	function map_empty() {
		var map = [:];
		assert_equal_strict(map[123], null);
		assert_true(std::empty(map));
	}
}