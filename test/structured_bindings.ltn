namespace sphinx::test {
	function s_bind_value() {
		var (i, b, f, s, t) = (1, true, 42.42, "xyz", (1, 0));
		assert_equal_strict(i, 1);
		assert_equal_strict(b, true);
		assert_equal_strict(f, 42.42);
		assert_equal_strict(s, "xyz");
		assert_equal_strict(t, (1,0));
	}



	function s_bind_value_ignore_extra() {
		var (i, b, f, s) = (1, true, 42.42, "xyz", null);
		assert_equal_strict(i, 1);
		assert_equal_strict(b, true);
		assert_equal_strict(f, 42.42);
		assert_equal_strict(s, "xyz");
	}



	function s_bind_type() {
		var (i, b, f, s, t) = (1, true, 42.42, "xyz", (1, 0));
		assert_equal_strict(reflect(expr i).type.name, "int");
		assert_equal_strict(reflect(expr b).type.name, "bool");
		assert_equal_strict(reflect(expr f).type.name, "float");
		assert_equal_strict(reflect(expr s).type.name, "string");
		assert_equal_strict(reflect(expr t).type.name, "tuple<int, int>");
	}


	
	function s_bind_type_ignore_extra() {
		var (i, b, f, s) = (1, true, 42.42, "xyz", null);
		assert_equal_strict(reflect(expr i).type.name, "int");
		assert_equal_strict(reflect(expr b).type.name, "bool");
		assert_equal_strict(reflect(expr f).type.name, "float");
		assert_equal_strict(reflect(expr s).type.name, "string");
	}



	function s_bind_value_multilevel() {
		var (i, b, (f, s)) = (1, true, (42.42, "xyz"));
		assert_equal_strict(i, 1);
		assert_equal_strict(b, true);
		assert_equal_strict(f, 42.42);
		assert_equal_strict(s, "xyz");
	}



	function s_bind_value_ignore_extra_multilevel() {
		var (i, b, (f, s)) = (1, true, (42.42, "xyz", null));
		assert_equal_strict(i, 1);
		assert_equal_strict(b, true);
		assert_equal_strict(f, 42.42);
		assert_equal_strict(s, "xyz");
	}



	function s_bind_type_multilevel() {
		var (i, b, (f, s)) = (1, true, (42.42, "xyz"));
		assert_equal_strict(reflect(expr i).type.name, "int");
		assert_equal_strict(reflect(expr b).type.name, "bool");
		assert_equal_strict(reflect(expr f).type.name, "float");
		assert_equal_strict(reflect(expr s).type.name, "string");
	}


	
	function s_bind_type_ignore_extra_multilevel() {
		var (i, b, (f, s)) = (1, true, (42.42, "xyz", null));
		assert_equal_strict(reflect(expr i).type.name, "int");
		assert_equal_strict(reflect(expr b).type.name, "bool");
		assert_equal_strict(reflect(expr f).type.name, "float");
		assert_equal_strict(reflect(expr s).type.name, "string");
	}
}