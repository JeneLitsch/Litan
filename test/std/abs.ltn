namespace sphinx::test {
	function std_abs() {
		assert_equal_strict(std::abs(true), 1);
		assert_equal_strict(std::abs('A'), 65);
		assert_equal_strict(std::abs(3), 3);
		assert_equal_strict(std::abs(-3), 3);
		assert_equal_strict(std::abs(3.5), 3.5);
		assert_equal_strict(std::abs(-3.5), 3.5);
	}
}