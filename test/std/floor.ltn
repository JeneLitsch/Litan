namespace sphinx::test {
	function std_floor() {
		assert_equal_strict(std::floor(3), 3);
		assert_equal_strict(std::floor(4.1), 4);
		assert_equal_strict(std::floor(4.8), 4);
		assert_equal_strict(std::floor(-3), -3);
		assert_equal_strict(std::floor(-4.1), -5);
		assert_equal_strict(std::floor(-4.8), -5);
	}
}