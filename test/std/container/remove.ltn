namespace sphinx::test {
	function std_remove_front_array() {
		var arr = [0,1,2,3];
		std::remove_front(arr);
		assert_equal_strict(arr, [1,2,3]);
	}



	function std_remove_front_string() {
		var str = "0123";
		std::remove_front(str);
		assert_equal_strict(str, "123");
	}



	function std_remove_back() {
		var arr = [0,1,2,3];
		std::remove_back(arr);
		assert_equal_strict(arr, [0,1,2]);
	}



	function std_remove_back_string() {
		var str = "0123";
		std::remove_back(str);
		assert_equal_strict(str, "012");
	}



	function std_remove_array() {
		var arr = [1,2,5,3];
		std::remove(arr, 2);
		assert_equal_strict(arr, [1,2,3]);
	}



	function std_remove_string() {
		var str = "1253";
		std::remove(str, 2);
		assert_equal_strict(str, "123");
	}



	function std_remove_map() { 
		var map = std::map();
		map[1] = "A";
		map[2] = "B";
		map[3] = "C";
		std::remove(map, 1);
		assert_equal_strict(map[1], null);
		assert_equal_strict(map[2], "B");
		assert_equal_strict(map[3], "C");
	}
}