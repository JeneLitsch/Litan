namespace sphinx::test {
	function std_is_noroutine_true() {
		test_name("std::noroutine() and std::is_noroutine()");
		assert_true(std::is_noroutine(std::noroutine()));
		assert_false(std::is_noroutine(1));
		assert_false(std::is_noroutine("ABC"));
		assert_false(std::is_noroutine(lambda => null));
		var cor = (lambda coroutine => null)();
		assert_false(std::is_noroutine(cor));
	}
}