namespace sphinx::test {
	function std_io_readln() {
		var in = std::strin("ABC\nXYZ\nPQR");
		assert_equal_strict(std::readln(in), "ABC");
		assert_equal_strict(std::readln(in), "XYZ");
	}



	function std_io_readln_type() {
		assert_equal_strict(reflect(expr std::readln(std::strin(""))).type.name, "string");
		assert_equal_strict(reflect(expr std::readln()).type.name, "string");
	}
}