namespace sphinx::test {
	
	var valid = [
		.{_+_} = lambda(self, other) => null,
		.{_-_} = lambda(self, other) => null,
		.{_*_} = lambda(self, other) => null,
		.{_/_} = lambda(self, other) => null,
		.{_%_} = lambda(self, other) => null,
		.{_**_} = lambda(self, other) => null,
		.{str} = lambda(self) => null,
	];
	
	var invalid_arity = [
		.{_+_} = lambda(self) => null,
		.{_-_} = lambda(self) => null,
		.{_*_} = lambda(self) => null,
		.{_/_} = lambda(self) => null,
		.{_%_} = lambda(self) => null,
		.{_**_} = lambda(self) => null,
		.{str} = lambda(self, other) => null,
	];
	
	var invalid_variadic = [ 
		.{_+_} = lambda(self, args...) => null,
		.{_-_} = lambda(self, args...) => null,
		.{_*_} = lambda(self, args...) => null,
		.{_/_} = lambda(self, args...) => null,
		.{_%_} = lambda(self, args...) => null,
		.{_**_} = lambda(self, args...) => null,
		.{str} = lambda(self, args...) => null,
	];
	
	var invalid_no_function  = [ .x = null ];


	namespace utils {
		function is_xable(test) {
			assert_true(test(valid));
			assert_false(test(invalid_arity));
			assert_false(test(invalid_variadic));
			assert_false(test(invalid_no_function));
		}
	}


	function std_is_addable()       => utils::is_xable(&std::is_addable(_))
	function std_is_subtractable()  => utils::is_xable(&std::is_subtractable(_))
	function std_is_multipliable()  => utils::is_xable(&std::is_multipliable(_))
	function std_is_divisible()     => utils::is_xable(&std::is_divisible(_))
	function std_is_modable()       => utils::is_xable(&std::is_modable(_))
	function std_is_exponential()   => utils::is_xable(&std::is_exponential(_))
	function std_is_stringyfiable() => utils::is_xable(&std::is_stringyfiable(_))
}