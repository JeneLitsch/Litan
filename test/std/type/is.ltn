		]);
	}



	function std_is_type_queue_float() {
		var q1 = std::queue();
		var q2 = std::queue();
		var q3 = std::queue();
		std::push(q2, 1);
		std::push(q3, 1.0);
		utils::test_is(<queue(float)>, [
			(null, false),
			(true, false),
			('A' , false),
			(1   , false),
			(1.0 , false),
			(""  , false),
			(q1  , true),
			(q2  , false),
			(q3  , true),
		]);
	}



	function std_is_type_map() {
		var map = std::map();
		map[1] = "";
		map[1.0] = "";
		utils::test_is(<map>, [
			(null, false),
			(true, false),
			('A' , false),
			(1   , false),
			(1.0 , false),
			(""  , false),
			(map , true),
		]);
	}



	function std_is_type_map_int_float() {
		var map1 = std::map();
		map1[1] = "";
		map1[1.0] = "";
		var map2 = std::map();
		map2[1] = 3.3;
		map2[2] = 4.4;
		utils::test_is(<map(int, float)>, [
			(null, false),
			(true, false),
			('A' , false),
			(1   , false),
			(1.0 , false),
			(""  , false),
			(map1, false),
			(map2, false),
		]);
	}
