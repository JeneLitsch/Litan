namespace test {

	function switch_int() {
		var fx = lambda (x) {
			switch (x) {
				case 1: return 1;
				case 2: return 2;
				case 3: return 3;
			}
			return 0;
		};

		var cases = [
			[[-1],0],
			[[1],1],
			[[2],2],
			[[3],3],
			[[4],0],
		];

		return test_all_cases("Switch(Int)", fx, cases);
	}



	function switch_float() {
		var fx = lambda (x) {
			switch (x) {
				case 1.5: return 1 + x;
				case 2.5: return 2 + x;
				case 3.5: return 3 + x;
			}
			return 0;
		};

		var cases = [
			[[-1],0],
			[[1.5], 2.5],
			[[2.5], 4.5],
			[[3.5], 6.5],
			[[4],0],
		];

		return test_all_cases("Switch(Float)", fx, cases);
	}



	function switch_mixed() {
		var fx = lambda (x) {
			switch (x) {
				case "ABC":   return 1;
				case [1,2,3]: return 2;
				case 'A':     return 3;
			}
			return 0;
		};

		var cases = [
			[[-1],         0],
			[["ABC"],      1],
			[[[1,2,3]],    2],
			[['A'],        3],
			[[std::cout()],0],
		];

		return test_all_cases("Switch(mixed)", fx, cases);
	}



	function sw1tch() {
		return switch_int() + switch_float() + switch_mixed();
	}
}