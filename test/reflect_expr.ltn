namespace sphinx::test {
	function reflect_expr() {
		assert_equal_strict(reflect(expr 20 + 22.0).type.name, "float");
		assert_equal_strict(reflect(expr [1] + [22.0]).type.name, "array<any>");
	}


	function reflect_type() {
		assert_equal_strict(reflect(type int).name, "int");
		assert_equal_strict(reflect(type array<string>).name, "array<string>");
	}
}