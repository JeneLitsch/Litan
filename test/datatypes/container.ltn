function testContainer(container) {
	std::push(container, 1);
	std::push(container, 2);
	std::push(container, 3);
	std::push(container, 4);
	std::println(std::peek(container));
	while(!std::empty(container)) {
		std::println(std::pop(container));
	}
}

function main() {
	std::println("Stack");
	testContainer(std::stack());
	std::println("");
	
	std::println("Queue");
	testContainer(std::queue());
	std::println("");

	std::println("Map");
	var map = std::map();
	std::insert(map, 1, "a");
	std::insert(map, 2, "b");
	std::insert(map, "c", "d");
	std::remove(map, "c");
	map["hello"] = "world";
	std::println("3 == " + std::size(map));
	std::println("a == " + map[1]);
	std::println("world == " + map["hello"]);
	std::println("b == " + std::at(map, 2));
	std::println("null == " + std::at(map, "c"));
	std::println("true == " + std::contains(map, 1));
	std::println("false == " + std::contains(map, 42));
}