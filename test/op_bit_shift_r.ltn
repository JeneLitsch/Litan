namespace sphinx::test {
	function shift_r() {
		test_name("operator >>");

		assert_equal(-1 >> 1, -1);
		assert_equal(2 >> 2, 0);
		assert_equal(64 >> 1, 32);
	}



	function shift_r_typename() {
		test_name("operator >> type");

		assert_type(<int>, 1 >> 1);
	}



	function shift_r_except() {
		test_name("operator >> except");

		assert_except(lambda => (4) >> (null));
		assert_except(lambda => (4.0) >> (2));
	}
}