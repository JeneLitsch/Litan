
function test(a, b) {
	std::println(a + " == " + b  + " -> " + (a == b));
	std::println(a + " != " + b  + " -> " + (a != b));
	std::println(a + " <= " + b  + " -> " + (a <= b));
	std::println(a + " >= " + b  + " -> " + (a >= b));
	std::println(a + " < "  + b  + " -> " + (a <  b));
	std::println(a + " > "  + b  + " -> " + (a >  b));
	std::println("");
}

function main() {
	test(1, -1);
	test(1.5, -1);
	test(2.1, 3.1);
	test(2, 3.5);
	test(42.0, 42);
}