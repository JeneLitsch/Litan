namespace sphinx::test {
	function switch_stmt_int() {
		var fx = lambda (x) switch(x) {
			case -3 => return 7;
			case -2 => return 8;
			case -1 => return 9;
			case 0 => return 10;
			case 1 => return 11;
			case 2 => return 12;
			case 3 => return 13;
			case 4 => return 14;
		};

		assert_equal(fx(-3), 7);
		assert_equal(fx(0), 10);
		assert_equal(fx(4), 14);
	}


	
	function switch_stmt_ther() {
		var fx = lambda (x) switch(x) {
			case 'A' => return 1;
			case "A" => return 2;
			case [] =>  return 3;
		};

		assert_equal(fx('A'), 1);
		assert_equal(fx("A"), 2);
		assert_equal(fx([]),  3);
		assert_equal(fx([1]), null);
	}



	function switch_stmt_default() {
		var fx = lambda (x) switch(x) {
			case 0 => return true;
			else   => return false;
		};

		assert_equal(fx(-3), false);
		assert_equal(fx(0), true);
		assert_equal(fx(4), false);
	}



	function switch_stmt_default_null() {
		var fx = lambda (x) switch(x) {
			case 0 => return true;
		};

		assert_equal(fx(-3), null);
		assert_equal(fx(0), true);
		assert_equal(fx(4), null);
	}



	function switch_stmt_scope() {
		var a = 42;
		switch(1) {
			case 1 => var a = 1337;
		}
		assert_equal(a, 42);
	}
}