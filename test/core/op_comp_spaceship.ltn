namespace sphinx::test {
    function space_ship() {
		test_name("operator: <=>");

		assert_equal(-1 <=> 1, -1);
		assert_equal(1 <=> 1, 0);
		assert_equal(1 <=> -1, 1);
		assert_equal('A' <=> 'A', 0);
		assert_equal('A' <=> 'Z', -1);
		assert_equal('Z' <=> 'A', 1);
		assert_equal("A" <=> "B", -1);
		assert_equal("C" <=> "B", 1);
		assert_equal("a" <=> "A", 1);
		assert_equal("zzz" <=> "zzz", 0);
		assert_equal([1,2,3] <=> [1,2,3], 0);
		assert_equal([1,2] <=> [1,2,3], -1);
		assert_equal([1,2,4] <=> [1,2,3], 1);
    }



	function space_ship_type() {
		test_name("operator: <=> type");

		assert_type(std::Int, 0.1 <=> 1.1);
		assert_type(std::Int, 1 <=> 1);
		assert_type(std::Int, 1 <=> "0");
	}
}