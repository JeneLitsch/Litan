namespace sphinx::test {
	function nulltest() {
		assert_equal(?null, false);
		assert_equal(?1,    true);
		assert_equal(?true, true);
		assert_equal(?[],   true);
	}



	function nulltest_type() {
		assert_equal(reflect(expr ?null).type.name, "bool");
		assert_equal(reflect(expr ?1).type.name, "bool");
		assert_equal(reflect(expr ?true).type.name, "bool");
		assert_equal(reflect(expr ?[]).type.name, "bool");
	}



	function nulltest_typename() {
		assert_equal(std::typename(?null), "bool");
		assert_equal(std::typename(?1), "bool");
		assert_equal(std::typename(?true), "bool");
		assert_equal(std::typename(?[]), "bool");
	}
}