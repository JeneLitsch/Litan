namespace op {
    function eql(sphinx) {
        var cases = [
			sphinx::assert_result(["Hello", "Hello"], true),
			sphinx::assert_result(["Hello", "World"], false),
			sphinx::assert_result([0, false], true),
			sphinx::assert_result([std::null_v(), true], false),
			sphinx::assert_result([std::cout(), std::cout()], true),
			sphinx::assert_result(["123", "123"], true),
		];

		var fx = lambda (a, b) => a == b;

		sphinx::run_test(sphinx, "Operator ==", fx, cases);
    }
}