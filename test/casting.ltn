function main() {
	std::println(std::bool(0));
	std::println(std::bool(1));
	std::println(std::bool(std::null()));

	std::println(std::int("-1337.3"));
	std::println(std::int(1.5));
	std::println(std::int(-42));

	std::println(std::float("-1337.3"));
	std::println(std::float(1.5));
	std::println(std::float(-42));

	std::println(std::string("\"" + std::null() + "\""));
}