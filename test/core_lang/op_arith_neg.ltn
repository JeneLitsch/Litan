namespace op {
	function neg(sphinx) {
		var cases = [
			sphinx::assert_result([1], -1), 
			sphinx::assert_result([-1], 1), 
			sphinx::assert_result([true], -1), 
			sphinx::assert_result([3.5], -3.5), 
			sphinx::assert_fail(["a"]),
			sphinx::assert_fail([[]]),
		];
		
		var fx    = lambda (x) => -x;

		sphinx::run_test(sphinx, "Operator -x", fx, cases);
	}
}