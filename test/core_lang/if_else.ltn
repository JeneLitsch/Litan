namespace core {
    function if_else(sphinx) {
        var fx = lambda (condition, a, b) {
			if(condition) {
				return a;
			}
			else return b;
		};

		var cases = [
			sphinx::assert_result([true, 1, 2], 1), 
			sphinx::assert_result([false, 1, 2], 2), 
			sphinx::assert_result([null, 1, 2], 2), 
			sphinx::assert_result([1, 1, 2], 1), 
		];

		sphinx::run_test(sphinx, "if else", fx, cases);
    }
}