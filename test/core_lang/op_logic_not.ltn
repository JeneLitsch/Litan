namespace op {
    function lognot(sphinx) {
        var cases = [
			sphinx::assert_result([true], false),
			sphinx::assert_result([false], true),
			sphinx::assert_result([null], true),
			sphinx::assert_result(["Hello World"], false),
			sphinx::assert_result([2], false),
			sphinx::assert_result([0], true),
			sphinx::assert_result([-1], false),
			sphinx::assert_result([-1.5], false),
			sphinx::assert_result([-0.0], true),
		];

		var fx = lambda(x) => !x;

		sphinx::run_test(sphinx, "Operator !", fx, cases);
    }
}