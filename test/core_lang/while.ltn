namespace core {
    function while_loop(sphinx) {
		var fx = lambda(arr) {
            var i = 0;
			var value = 0;
            while(i < std::size(arr)) {
				value = value + arr[i];
                i=i+1;
			}
			return value;
		};

		var cases = [
			sphinx::assert_result([[1,2,3]], 6),
			sphinx::assert_result([[10,20,30]], 60),
			sphinx::assert_result([[10,20,30, -20]], 40),
		];

		sphinx::run_test(sphinx, "while", fx, cases);
	}
}