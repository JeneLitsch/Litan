namespace op {
    function logand(sphinx) {
        var fx = lambda(a, b) => a && b;

        var cases = [
            sphinx::assert_result([true, true], true),
            sphinx::assert_result([true, false], false),
            sphinx::assert_result([false, true], false),
            sphinx::assert_result([false, false], false),
        ];

        sphinx::run_test(sphinx, "Operator &&", fx, cases);
    }
}