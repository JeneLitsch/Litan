namespace op {
    function uneql_value(sphinx) {
        var cases = [
			sphinx::assert_result(["Hello", "Hello"], false),
			sphinx::assert_result(["Hello", "World"], true),
			sphinx::assert_result([0, false], false),
			sphinx::assert_result([std::null_v(), true], true),
			sphinx::assert_result([std::cout(), std::cout()], false),
			sphinx::assert_result(["123", "123"], false),
		];

		var fx = lambda (a, b) => a != b;

		sphinx::run_test(sphinx, "Operator !=", fx, cases);
    }



	function uneql_type(sphinx) {
		var cases = [
			type_check(decltype(1 != "0"), "bool"),
		];

		sphinx::run_test(sphinx, "Operator != type", lambda(x) => x, cases);
	}



	function uneql(sphinx) {
		uneql_value(sphinx);
		uneql_type(sphinx);
	}
}