function main() {
	var sphinx = sphinx::init(std::cout());

	operators::add(sphinx);
	operators::sub(sphinx);
	operators::mlt(sphinx);
	operators::div(sphinx);
	operators::mod(sphinx);
	operators::pow(sphinx);
	operators::neg(sphinx);

	operators::shift_l(sphinx);
	operators::shift_r(sphinx);

	operators::bitor(sphinx);
	operators::bitand(sphinx);
	operators::bitxor(sphinx);
	operators::bitnot(sphinx);

	operators::eql(sphinx);
	operators::uneql(sphinx);
	operators::less(sphinx);
	operators::less_eql(sphinx);
	operators::greater(sphinx);
	operators::greater_eql(sphinx);

	operators::space_ship(sphinx);

	operators::approx(sphinx);
	operators::not_approx(sphinx);

	operators::ternary(sphinx);
	operators::elvis(sphinx);
	operators::nullco(sphinx);

	sphinx::interprete_results(sphinx);
}