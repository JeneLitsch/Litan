function fx() {
	return 1;
}

namespace foo {
	function fx() {
		return 3;
	}

	namespace bar {
		function fx() {
			return 6;
		}
	}
}

namespace bar {
	function fx() {
		return 7;
	}
}

namespace test {
	namespace foo {
		namespace bar {
			function fx() {
				return 5;
			}
		}

		function fx() {
			return 4;
		}
	}

	function fx() {
		return 2;
	}
}

namespace test {
	function identity(a) {
		return a;
	}
	
	function namespacing() {
		var cases = [
			[[::fx()], 1], 
			[[fx()], 2], 
			[[::test::fx()], 2], 
			[[::foo::fx()], 3],
			[[foo::fx()], 4],
			[[test::foo::fx()], 4],
			[[foo::bar::fx()], 5],
			[[::test::foo::bar::fx()], 5],
			[[::foo::bar::fx()], 6],
		];

		return test_all_cases("namespace", &identity(_), cases);
	}
}