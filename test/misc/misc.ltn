function main() {
	std::println("=== String ===");
	var str = "HX World";
	std::remove(str, 1);
	std::insert(str, 1, "ello");
	std::println(str);
	std::println("");

	std::println("=== Array ===");
	var arr = [];
	std::insert_front(arr, 1);
	std::insert_back(arr, 3);
	std::insert(arr, 1, 2);
	std::println(arr);

	const a = 123456789;
	const b = a;
	std::println(b);

	std::println(42 + "s");
	std::println("s" + 42 );
}