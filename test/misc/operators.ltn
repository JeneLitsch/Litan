function equation(a, b, r, op) {
	std::println(a + " " +  op + " " + b + " = " + r);
}

function test(a, b) {
	equation(a, b, a+b, "+");
	equation(a, b, a-b, "-");
	equation(a, b, a*b, "*");
	equation(a, b, a/b, "/");
	equation(a, b, a%b, "%");
	std::println("");
}

function main() {
	test(true, true);
	test(false, 2.5);
	test(-1, 1.5);
	std::println('A' + 1);
}