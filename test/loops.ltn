namespace test {
	function test_for_alt1() {
		var fx = lambda(begin, end) {
			var sum = 0;
			for i (begin, end) {
				sum += i;
			}
			return sum;
		};

		var cases = [
			[[1, 3], 3],
			[[-1, 3], 2],
			[[3, 1], 0],
			[[-3, -1], -5],
			[[-3, 3], -3],
			[[-5, 6], 0],
			[[0, 10], 45],
			[[0, 11], 55],
			[[-1, 10], 44],
			[[-1000, -10000], 0],
		];

		return test_all_cases("for", fx, cases); 
	}

	function loops() {
		return
			test_for_alt1();
	}
}