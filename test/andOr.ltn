function t() {
	std::println("true");
	return true;
}

function f() {
	std::println("false");
	return false;
}

function main() {
	std::println("AND &&");
	if(f() && t()) {
		std::println("Both true");
	}
	else {
		std::println("not true");
	}

	std::println("OR ||");
	if(f() || t()) {
		std::println("at least one true");
	}
	else {
		std::println("both not true");
	}
}