namespace sphinx::test {
	function var_any() {
		var x;
		assert_equal_strict(x, null);
	}



	function var_bool() {
		var x = true;
		assert_equal_strict(x, true);
	}



	function var_int() {
		var x = 0;
		assert_equal_strict(x, 0);
	}


	
	function var_float() {
		var x =  0.0;
		assert_equal_strict(x, 0.0);
	}



	function var_string() {
		var x = "";
		assert_equal_strict(x, "");
	}



	function var_array() {
		var x = [1,2,3];
		assert_equal_strict(x, [1,2,3]);
	}
}