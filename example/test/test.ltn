function fxTrue() -> Void {
	->ltn::println(true);
}

function fxFalse() -> Void {
	->ltn::println(false);
}


struct Struct1 {
	var Int x;
	var Int y;
	var Int z;
}

struct Struct2 {
	var Struct1 s1;
}

function main() -> Void {
	var Struct2 s2 = ->Struct2(->Struct1(1, 2, 3));
	->ltn::println(s2.s1.x);
	->ltn::println(s2.s1.y);
	->ltn::println(s2.s1.z);
	return;
}
