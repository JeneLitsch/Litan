struct Bar {
	var int x;
}

struct Foo {
	var Bar bar;
	var int test;
}

fnx fx(int a, int b) Foo {
	var Foo foo = new Foo(new Bar(a), b);
	return foo;
}

fnx main() voi {
	var Foo foo = ->fx(42, 1337);
	var Foo foo2 = copy foo;
	foo.test = 7331;
	->print(foo2.bar.x);
	->print(foo2.test);

	return;
}