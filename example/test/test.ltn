struct Bar {
	var int x;
}

struct Foo {
	var Bar bar;
	var int test;
}

fnx main() voi {
	var Foo foo;
	foo = new Foo();
	foo.bar = new Bar();
	foo.bar.x = 42;
	foo.test = 1337;
	->print(foo.bar.x);
	del foo.bar;
	->print(foo.test);
	
	foo.bar = new Bar();
	foo.bar.x = 69;
	->print(foo.bar.x);

	asm {
		"size"
		"print"
	}
	return;
}