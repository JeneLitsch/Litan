function solve(a, b, op) {
	if(op == "+") {
		return a + b;
	}
	if(op == "-") {
		return a - b;
	}
	if(op == "*") {
		return a * b;
	}
	if(op == "/") {
		return a / b;
	}
	ltn::println("Invalid operator" + op);
}

function main() {
	var a = ltn::readfloat();
	var op = ltn::read();
	var b = ltn::readfloat();
	var result = solve(a, b, op);
	ltn::println("Result: " + a + op + b + " = " + result);
}