namespace ltn {
	typedef Array;

	function Array() -> ltn::Array {
		var ltn::Array array;
		asm {
			"array::new"
			"store 0"
		}
		return array;
	}

	function pushBack(ltn::Array array, Raw value) -> Void {
		asm {
			"load 0"
			"load 1"
			"array::pushb"
		}
		return;
	}

	function at(ltn::Array array, Int idx) -> Raw {
		asm {
			"load 0"
			"load 1"
			"array::get"
			"return"
		}
	}
}