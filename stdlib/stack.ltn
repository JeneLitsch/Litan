namespace ltn {
	typedef Stack;

	function Stack() -> ltn::Stack {
		asm { "heap::allocate::stack" "return" }
	}

	function push(ltn::Stack stack, Raw value) -> Void
		asm { "load 0" "load 1" "stack::push" }


	function pop(ltn::Stack stack) -> Raw
		asm { "load 0" "stack::pop" "return" }


	function top(ltn::Stack stack) -> Raw
		asm { "load 0" "stack::top" "return" }


	function empty(ltn::Stack stack) -> Bool 
		asm { "load 0" "stack::empty" "return" }

	function size(ltn::Stack stack) -> Int 
		asm { "load 0" "stack::size" "return" }
}