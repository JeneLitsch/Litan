typedef Void;
typedef Int;
typedef Float;
typedef Raw;
typedef Pointer;

function Int(Raw number) -> Int
	asm { "load 0" "return" }

function Int(Float number) -> Int
	asm { "load 0" "casti" "return" }

function Float(Raw number) -> Float
	asm { "load 0" "return" }

function Float(Int number) -> Float
	asm { "load 0" "castf" "return" }

function destroy(Pointer ptr) -> Void
	asm { "load 0" "heap::del" }

function exists(Pointer ptr) -> Int
	asm { "load 0" "heap::exist" "return" }