namespace std {
	function whole(array) {
		return range(array, 0, std::size(array));
	}


	function first_n(array, n) {
		return range(array, 0, n);
	}


	function last_n(array, n) {
		var size = std::size(array);
		return range(array, size - n, size);
	}


	asm range(array, begin, end) {
		"newrange"
		"return"
	}
}