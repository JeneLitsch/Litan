
fnx newArray() arr {
	arr array;
	asm {
		"array::new"
		"store 1"
	}
	return array;
}

fnx delArray(arr array) voi {
	asm {
		"load 0"
		"array::del"
	}
	return;
}

fnx addToArray(arr array, int value) voi {
	asm {
		"load 0"
		"load 1"
		"array::add"
	}
	return;
}

fnx readArrayI (arr array, int idx) int {
	asm {
		"load 0"
		"load 1"
		"array::get"
		"return"
	}
}
