typedef Array;

function Array() -> Array {
	var Array array;
	asm {
		"array::new"
		"store 0"
	}
	return array;
}

function destroy(Array array) -> Void {
	asm {
		"load 0"
		"heap::del"
	}
	return;
}

function pushBack(Array array, Raw value) -> Void {
	asm {
		"load 0"
		"load 1"
		"array::pushb"
	}
	return;
}

function at(Array array, Int idx) -> Raw {
	asm {
		"load 0"
		"load 1"
		"array::get"
		"return"
	}
}
