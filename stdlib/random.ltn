namespace std {
	namespace random {
		asm mersenne() {
			"newrng 0"
			"duplicate"
			"random 0"
			"return"
		}
		
		asm seed(rng) {
			"random 0"
		}

		asm seed(rng, seed) {
			"random 1"
		}

		asm rand(rng) {
			"random 10"
			"return"
		}

		asm rand_int(rng, min, max) {
			"random 11"
			"return"
		}

		asm rand_float(rng, min, max) {
			"random 12"
			"return"
		}

		function decision(rng, p) => rand_float(rng, 0, 1) < p
	}
}