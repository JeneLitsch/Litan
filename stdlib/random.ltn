namespace std {
	namespace random {
		asm mersenne() {
			"newrng 00"
			"return"
		}

		asm mersenne(seed) {
			"newrng 01"
			"return"
		}
		
		asm rand(rng) {
			"random 10"
			"return"
		}

		asm rand_int(rng, min, max) {
			"random 11"
			"return"
		}

		asm rand_float(rng, min, max) {
			"random 12"
			"return"
		}

		function decision(rng, p) => rand_float(rng, 0, 1) < p
	}
}