namespace ltn {
	function randInt(Int a, Int b) -> Int {
		asm { "load 0" "load 1" "rand::int" "return" }
	}

	function randFloat(Float a, Float b) -> Float {
		asm { "load 0" "load 1" "rand::float" "return" }
	}

	function randNorm() -> Float {
		asm { "rand::norm" "return" }
	}

	function randUnit() -> Float {
		asm { "rand::unit" "return" }
	}
}