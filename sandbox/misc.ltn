global x;

namespace foo {
	global x;
	function get_x() => global x
}

function get_x() => global x

function set_globals() {
	global x = "Hello";
	global foo::x = "World";
}

function main(args) {
	set_globals();
	std::println(get_x());
	std::println(foo::get_x());
	std::println(global x + global foo::x);
}