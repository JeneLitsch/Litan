function map(arr) {
	var map = std::map();
	for(elem : arr) {
		map[elem[0]] = elem[1];
	}
	return map;
}



function zip(ranges) {
	var r = [];
	for(i : std::zipped(ranges)) {
		std::insert_back(r, i);
	}
	return r;
}



function main(args) {
	var a = [1,2,3,4,5];
	var b = ["A","B","C", "D"];
	for(e : std::zipped((a, b))) {
		std::println(e);
	}
	std::println(zip((a,b)));
}