function main(args) {
	var obj = [
		.x = 42,
		.{add} = lambda(self, other) {
			var clone = std::clone(self);
			clone.x = clone.x + other;
			return clone;
		},
		.{sub} = lambda(self, other) {
			var clone = std::clone(self);
			clone.x = clone.x - other;
			return clone;
		},
	];

	std::println((obj + 2).x);
	std::println((obj - 2).x);
	std::println((obj + 1).x);
	std::println((obj - 1).x);
}