define a = 41;

function main() {
	var arr = [1, 2, 3];
	for (i : 0 -> std::size(arr))  {
		var x = arr[i];
		std::println(x);
	}
	std::println("");

	for (i : 0 -> 10 {-})  {
		std::println(i);
	}
	std::println("");

	for (i : 0 -> 10 : 2 {+})  {
		std::println(i);
	}
	std::println("");

	for (i : 0 => 10 : 2)  {
		std::println(i);
	}
	std::println("");

	for (i : 0 => 10 : 2 {-})  {
		std::println(i);
	}
	std::println("");
}