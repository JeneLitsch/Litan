function add(a : int, b : int) const -> int {
    return a + b;
}

function main() extern -> int  {
	var foo;
	var bar : int = add(21,21);
	var baz : string = "Hello World";
	std::println(foo);
	std::println(bar);
	std::println(baz);
	var x : char = 'A';
	std::println(x);
	var pi = 3;
	std::println(std::pi);
	std::println(pi);
}