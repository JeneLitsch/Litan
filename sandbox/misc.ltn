function map(arr) {
	var map = std::map();
	for(elem : arr) {
		map[elem[0]] = elem[1];
	}
	return map;
}



function zip(a, b) {
	var r = [];
	var size = std::min(std::size(a), std::size(b));
	for(i : std::range(0, size)) {
		std::insert_back(r, (a[i], b[i]));
	}
	return r;
}




function main(args) {
	var a = [1,2,3,4,5];
	var b = ["A","B","C", "D"];
	for(e : std::combined((a, b))) {
		std::println(e);
	}
}